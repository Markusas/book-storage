<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>lt.book.storage</groupId>
    <artifactId>book-storage</artifactId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <groupId>lt.book.storage.app</groupId>
  <artifactId>book-storage-app</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>war</packaging>
  <name>book-storage-app Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>lt.book.storage.service</groupId>
      <artifactId>book-storage-service</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
